{
  "name": "push-service",
  "version": "1.0.0",
  "description": "Building a simple one dirctional push service in NodeJS leveraging Server-Sent-Events rather than websockets. (Proof of concept)",
  "main": "server.js",
  "scripts": {
    "start": "NODE_PATH=. node -r dotenv/config server.js",
    "dev": "cross-env NODE_ENV=development webpack --progress --hide-modules -w",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
